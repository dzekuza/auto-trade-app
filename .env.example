### Copy this file to .env and fill in values for your environment
# RPC endpoint for the EVM network (e.g. Infura or Alchemy)
RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
# Private key of your trading wallet (DO NOT COMMIT .env)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY
# Address of the DEX router you want to use (Uniswap V2 by default on mainnet)
DEX_ROUTER_ADDRESS=0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f
# Slippage tolerance in basis points (e.g. 500 = 5%)
SLIPPAGE_BPS=500
# Number of minutes before a swap transaction expires
TX_DEADLINE_MINUTES=15